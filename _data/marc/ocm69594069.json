        "subfields": [
          {
            "g": "25."
          },
          {
            "t": "Aftermaths, 1892-1894 --"
          },
          {
            "g": "26."
          },
          {
            "t": "\"Be of good cheer -- we will be over it soon, 1893-1895 --"
          },
          {
            "g": "27."
          },
          {
            "t": "Sixty years old, 1895-1896 --"
          },
          {
            "g": "28. \"An"
          },
          {
            "t": "impregnable position,\" 1896-1898 --"
          },
          {
            "g": "29."
          },
          {
            "t": "\"We now want to take root,\" 1897-1898 --"
          },
          {
            "g": "30."
          },
          {
            "t": "The anti-imperialist, 1898-1899 --"
          },
          {
            "g": "31. \"The"
          },
          {
            "t": "richest man in the world,\" 1899-1901 --"
          },
          {
            "g": "32. \"The"
          },
          {
            "t": "saddest days of all,\" 1901 --"
          },
          {
            "g": "33. \"A"
          },
          {
            "t": "fine piece of friendship,\" 1902-1905 --"
          },
          {
            "g": "34."
          },
          {
            "t": "\"Apostle of peace,\" 1903-1904 --"
          },
          {
            "g": "35."
          },
          {
            "t": "\"Inveterate optimist,\" 1905-1906 --"
          },
          {
            "g": "36."
          },
          {
            "t": "Peace conference, 1907 --"
          },
          {
            "g": "37."
          },
          {
            "t": "Tariffs and treaties, 1908-1909 --"
          },
          {
            "g": "38."
          },
          {
            "t": "\"So be it,\" 1908-1910 --"
          },
          {
            "g": "39."
          },
          {
            "t": "The best laid schemes, 1909-1911 --"
          },
          {
            "g": "40."
          },
          {
            "t": "\"Be of good cheer,\" 1912-1913 --"
          },
