        "subfields": [
          {
            "t": "Introduction : American philanthropy if necessary but not necessarily American philanthropy --"
          },
          {
            "g": "pt. 1."
          },
          {
            "t": "Building foundations --"
          },
          {
            "g": "1."
          },
          {
            "t": "The business of benevolence --"
          },
          {
            "g": "2."
          },
          {
            "t": "The early years of American philanthropy in Canada : building schools, building Canada --"
          },
          {
            "g": "pt. 2."
          },
          {
            "t": "American philanthropy, imagined communities, and the structuring of the arts in Canada, 1927-1952 --"
          },
          {
            "g": "3."
          },
          {
            "t": "American philanthropy, \"cultural interpretation,\" and imagined communities in Canada --"
          },
          {
            "g": "4."
          },
          {
            "t": "The Carnegie Corporation, cultural philanthropy, and a new deal for the arts in Canada --"
          },
          {
            "g": "pt. 3."
          },
          {
            "t": "American philanthropy and Canadian letters --"
          },
          {
            "g": "5."
          },
          {
            "t": "American philanthropy and intellectual development in Canada, 1930-1957 --"
          },
